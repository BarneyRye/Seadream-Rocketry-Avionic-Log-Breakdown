{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build C Project",
      "type": "shell",
      "command": "gcc",
      "args": [
        "-Wall",
        "-Wextra",
        "-g",
        "*.c",
        "-o",
        "breakdown",
        "-lm"
      ],
      "group": { "kind": "build", "isDefault": true },
      "problemMatcher": ["$gcc"]
    }
  ]
}